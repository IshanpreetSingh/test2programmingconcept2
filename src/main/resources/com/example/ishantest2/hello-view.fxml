<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="766.0" prefWidth="462.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ishantest2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="eusername" text="Username" />
    <TextField fx:id="eExpid" text="ExpenseID" />
    <TextField fx:id="eExptyp" text="ExpenseType" />
    <TextField fx:id="eamt" text="Amount" />
    <TextField fx:id="edate" text="Date " />


    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Click Here" />
    <Button mnemonicParsing="false" onAction="#InsertData" text="Insert" />
    <Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
    <Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
    <Button mnemonicParsing="false" onAction="#LoadData" text="Load" />
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="expnseid" prefWidth="75.0" text="ExpenseID" />
            <TableColumn fx:id="expensetype" prefWidth="75.0" text="ExpenseType" />
            <TableColumn fx:id="amount" prefWidth="75.0" text="Amount" />
            <TableColumn fx:id="date" prefWidth="75.0" text="Date" />
        </columns>
    </TableView>
</VBox>
