<VBox alignment="CENTER" prefHeight="397.0" prefWidth="433.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ishantest2.Logincontroller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Pane prefHeight="355.0" prefWidth="393.0">
            <children>
                <TextField fx:id="email" layoutX="105.0" layoutY="123.0" prefHeight="25.0" prefWidth="187.0" />
                <PasswordField fx:id="pswd" layoutX="105.0" layoutY="165.0" prefHeight="25.0" prefWidth="187.0" />
                <Label layoutX="60.0" layoutY="127.0" text="Email" />
                <Label layoutX="50.0" layoutY="165.0" text="Password" />
                <Button layoutX="152.0" layoutY="218.0" mnemonicParsing="false" onAction="#LoginClick" prefHeight="25.0" prefWidth="92.0" text="Login" />
                <Label fx:id="msg" layoutX="62.0" layoutY="118.0" prefHeight="17.0" prefWidth="261.0" textFill="#d31b1b" />
            <TextField fx:id="username" layoutX="105.0" layoutY="77.0" prefHeight="25.0" prefWidth="187.0" />
            <Text layoutX="47.0" layoutY="94.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
            <TextField fx:id="id" layoutX="108.0" layoutY="31.0" prefHeight="25.0" prefWidth="187.0" />
            <Text layoutX="64.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID" />
            </children>
        </Pane>
    </children>

</VBox>
